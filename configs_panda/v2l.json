{
  "network": "TimmUnetPANDA",
  "encoder_params": {
    "encoder": "tf_efficientnetv2_l_in21k",
    "in_chans": 3,
    "drop_path_rate": 0.2,
    "pretrained": true,
    "channels_last": true
  },
  "crop_size": 224,
  "multiplier": 64,
  "optimizer": {
    "train_bs": 2,
    "val_bs": 1,
    "type": "SGD",
    "learning_rate": 0.003,
    "weight_decay": 1e-4,
    "nesterov": true,
    "momentum": 0.9,
    "schedule": {
      "type": "cosine",
      "mode": "step",
      "epochs": 20,
      "params": {
        "eta_min": 4e-4
      }
    }
  },
   "losses": [
    {
      "name": "msk",
      "type": "CrossEntrophy",
      "weight": 3.0,
      "display": true,
      "params": {
        "field": "mask"
      }
    },
    {
      "name": "cls",
      "type": "CrossEntrophy",
      "weight": 0.0,
      "display": true,
      "params": {
        "field": "local_score"
      }
    }
  ]
}
