{
  "network": "TimmUnet",
  "encoder_params": {
    "encoder": "revswinv2_tiny_window16_256_xview",
    "in_chans": 2,
    "drop_path_rate": 0.2,
    "pretrained": "ckpts/swinv2_tiny_window16_256.pth",
    "channels_last": true
  },
  "crop_size": 1024,
  "multiplier": 64,
  "optimizer": {
    "train_bs": 8,
    "val_bs": 4,
    "type": "SGD",
    "learning_rate": 0.0003,
    "weight_decay": 1e-4,
    "nesterov": true,
    "momentum": 0.9,
    "classifier_lr": 0.003,
    "schedule": {
      "type": "cosine",
      "mode": "step",
      "epochs": 120,
      "params": {
        "eta_min": 3e-5
      }
    }
  },
   "losses": [
    {
      "name": "msk_v",
      "type": "ComboLossCalculator",
      "weight": 1.0,
      "display": true,
      "params": {
        "field": "vessel_mask",
        "weights": {
          "bce": 1,
          "focal": 3,
          "dice": 1
        }
      }
    },
    {
      "name": "msk_f",
      "type": "ComboLossCalculator",
      "weight": 2.0,
      "display": true,
      "params": {
        "field": "fishing_mask",
        "weights": {
          "bce": 1,
          "focal": 3,
          "dice": 1
        }
      }
    },
    {
      "name": "center",
      "type": "CenterLossCalculator",
      "weight": 3e-7,
      "display": true,
      "params": {
      }
    },
    {
      "name": "length",
      "type": "LengthLoss",
      "weight": 2,
      "display": true,
      "params": {
      }
    }
   
  ]
}
